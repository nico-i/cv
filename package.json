{
  "name": "cv",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "IS_LOCAL_ENV=true && tinacms dev -c \"netlify dev -c 'astro dev' --verbose\"",
    "dev:print": "IS_LOCAL_ENV=true && tinacms dev -c \"astro dev\"",
    "gen:i18n": "npx astro-i18next generate",
    "build": "tinacms build && astro check && astro build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . && npm run format",
    "lint:types": "tsc --noEmit --incremental false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "print": "ts-node ./scripts/print-pdf.js http://localhost:8888",
    "prepare": "husky && echo \"#!/bin/sh\nnpx --no -- commitlint --edit \\$1\" > .husky/commit-msg"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.2",
    "@octokit/rest": "^21.0.2",
    "abstract-level": "^1.0.4",
    "astro": "^4.16.13",
    "astro-i18next": "^1.0.0-beta.21",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "module-error": "^1.0.2",
    "mongodb": "^6.16.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.15",
    "tinacms": "^2.5.0",
    "tinacms-authjs": "^8.0.0",
    "tinacms-gitprovider-github": "^2.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.15.0",
    "@netlify/functions": "^2.8.2",
    "@nico-i/eslint-config": "^2.7.0",
    "@nico-i/prettier-config": "^1.0.0",
    "@tinacms/cli": "^1.6.12",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/react-dom": "^18.3.1",
    "commitlint-plugin-spend": "^1.0.1",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "netlify-cli": "^17.37.2",
    "prettier": "3.4.1",
    "puppeteer": "^23.10.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
